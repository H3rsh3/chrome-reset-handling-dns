{
    "manifest_version": 3,
    "name": "connection reset handling",
    "version": "4.0",
    "description": "Displays a custom page for stalled connections.",
    "permissions": [ 
      "storage",
      "webRequest",
      "dns"
    ],
    "host_permissions": ["http://*/*", "https://*/*"],
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "options.html" 
  }